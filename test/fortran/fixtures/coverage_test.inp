;
;EXE-FILE: APM-MODEL.EXE
;
; FILE PATHS AND NAMES
;PVT-PATH:        "(PATH/TO/PVT-DATA.*)"
;APER-MAP PATH:   "(PATH/TO/FILE.*)"
SUMMARY-PATH:    ./coverage_test_log.txt
STAT-FILE PATH:  ./coverage_test_stats.csv
APER-FILE PATH:  ./coverage_test_map.csv
FLOW-FILE PATH:  ./coverage_test_flow.csv
PRESS-FILE PATH: ./coverage_test_press.csv
VTK-FILE PATH:   ./coverage_test_data.vtk
OVERWRITE EXISTING FILES
;
; BOUNDARY CONDITIONS
FRAC-PRESS: 100 PA ;HITTING THE OLD KEYWORDS AS WELL
INLET-PRESS: 100 PA  
;INLET-RATE: 1.0 MM^3/SEC
OUTLET-PRESS: 0 PA 
;OUTLET-RATE: 1.0 MM^3/SEC
OUTFLOW-SIDE: TOP ;DEPRECATED
OUTLET-SIDE:  TOP
;
; MODEL PROPERTIES
FLUID-TYPE:    LIQUID
FLUID-DENSITY: 1000 KG/M^3
FLUID-VISCOSITY: 1.0 PA*SEC
MAXIMUM MAP DIMENSION: 250
STD-TEMP:      273.15 K
STD-PRESS:       1.00 ATM
MANIFOLD: FALSE ;DEPRECATED 
SOLVER TYPE: D4-GAUSS ;DEPRECATED
;
; OTHER PARAMETERS
MAP AVERAGING FACTOR: 10
VOXEL SIZE: 20.0 MICRONS
ROUGHNESS REDUCTION: 0.0 ;IN VOXELS
CALCULATE PERCENTILES: 0,1,5,10,15,20,25,30,40,50,60,70,75,80,85,90,95,99,100
HIGH-MASK: 100.00   ;IN VOXELS
LOW-MASK:    0.0001 ;IN VOXELS
;
; DEFINE SPECIFIC OUTPUT UNITS TO USE
; REQUIRED FIELD ORDER: PRESSURE, DISTANCE, FLOW RATE
OUTPUT-UNITS: PA,MM,MM^3/SEC
